{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [
      "jsdoc"
    ]
  },
  "plugins": ["node_modules/jsdoc-vuejs"],
  "source": {
    "include": [
      "./src/packages"

    ],
    "includePattern": "\\.(vue|js)$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "opts": {
    "destination": "public",
    "template": "node_modules/minami"
  }
}